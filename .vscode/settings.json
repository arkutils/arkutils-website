{
    "files.exclude": {
        ".svelte-kit": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        }
    },
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        }
    },
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        }
    },
    "[json]": {
        "editor.formatOnSave": true
    },
    "[jsonc]": {
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "arkutils",
        "Beyla",
        "Bossopedia",
        "Cryopod",
        "Dinos",
        "dont",
        "Fenris",
        "Fenrisúlfr",
        "Fjordur",
        "Hati",
        "Kaiju",
        "Lysrix",
        "Megapithicus",
        "modid",
        "Mults",
        "PVPVE",
        "Skoll",
        "Steinbjorn",
        "sveltejs",
        "Unsubscriber",
        "Valguero"
    ],
    "terminal.integrated.wordSeparators": " []{}',\"`─‘’"
}
